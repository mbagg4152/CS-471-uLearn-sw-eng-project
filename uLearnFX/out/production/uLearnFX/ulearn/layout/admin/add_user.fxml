<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?><?import javafx.scene.control.*?><?import javafx.scene.layout.*?><?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="ulearn.controllers.ToolCont"
    prefHeight="400.0" prefWidth="600.0">
  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
    <GridPane prefHeight="370.8" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
      </rowConstraints>
      <TextField fx:id="txtNewUuid" maxWidth="1.7976931348623157E308" minHeight="25.0" minWidth="-Infinity"
          promptText="User ID">
        <GridPane.margin>
          <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
        </GridPane.margin>
        <padding>
          <Insets left="10.0" right="10.0"/>
        </padding>
      </TextField>
      <TextField fx:id="txtNewFirst" maxWidth="1.7976931348623157E308" minHeight="25.0" promptText="First Name"
          GridPane.rowIndex="1">
        <padding>
          <Insets left="10.0" right="10.0"/>
        </padding>
        <GridPane.margin>
          <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
        </GridPane.margin>
      </TextField>
      <TextField fx:id="txtNewLast" maxWidth="1.7976931348623157E308" minHeight="25.0" promptText="Last Name"
          GridPane.rowIndex="2">
        <padding>
          <Insets left="10.0" right="10.0"/>
        </padding>
        <GridPane.margin>
          <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
        </GridPane.margin>
      </TextField>
      <TextField fx:id="txtNewPwd" maxWidth="1.7976931348623157E308" minHeight="25.0"
          promptText="Default Password" GridPane.rowIndex="4">
        <padding>
          <Insets left="10.0" right="10.0"/>
        </padding>
        <GridPane.margin>
          <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
        </GridPane.margin>
      </TextField>
      <Button fx:id="newUserBtn" mnemonicParsing="false" onAction="#makeNewUser" prefWidth="100.0"
          text="Create User" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4"/>
      <DatePicker fx:id="newGradDate" maxWidth="1.7976931348623157E308" promptText="Expected Grad Date"
          GridPane.rowIndex="3">
        <opaqueInsets>
          <Insets/>
        </opaqueInsets>
        <GridPane.margin>
          <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
        </GridPane.margin>
      </DatePicker>
      <RadioButton fx:id="adminBtn" mnemonicParsing="false" text="Admin" GridPane.columnIndex="1"
          GridPane.rowIndex="1">
        <GridPane.margin>
          <Insets bottom="10.0" left="30.0" right="10.0" top="10.0"/>
        </GridPane.margin>
      </RadioButton>
      <RadioButton fx:id="eduBtn" mnemonicParsing="false" text="Educator" GridPane.columnIndex="1"
          GridPane.rowIndex="2">
        <GridPane.margin>
          <Insets bottom="10.0" left="30.0" right="10.0" top="10.0"/>
        </GridPane.margin>
        <toggleGroup>
          <ToggleGroup fx:id="group"/>
        </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="studentBtn" mnemonicParsing="false" text="Student" toggleGroup="$group"
          GridPane.columnIndex="1" GridPane.rowIndex="3">
        <GridPane.margin>
          <Insets bottom="10.0" left="30.0" right="10.0" top="10.0"/>
        </GridPane.margin>
      </RadioButton>
      <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="New User Type"
          GridPane.columnIndex="1">
        <GridPane.margin>
          <Insets bottom="10.0" left="30.0" right="10.0" top="10.0"/>
        </GridPane.margin>
        <font>
          <Font name="System Bold" size="14.0"/>
        </font>
      </Label>
    </GridPane>
  </AnchorPane>
</AnchorPane>
