<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane prefHeight="262.0" prefWidth="310.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="ulearn.controllers.InitCont">
	<Label fx:id="title" layoutX="98.0" layoutY="39.0" maxHeight="Infinity" maxWidth="Infinity"
	       nodeOrientation="LEFT_TO_RIGHT" text="uLearn Login">
		<font>
			<Font name="Corbel Bold" size="20.0"/>
		</font>
	</Label>
	<Label fx:id="unameLabel" layoutX="33.0" layoutY="107.0" maxHeight="Infinity" maxWidth="Infinity"
	       nodeOrientation="LEFT_TO_RIGHT" text="Username">
		<font>
			<Font name="Corbel" size="14.0"/>
		</font>
	</Label>
	<Label fx:id="pwdLabel" layoutX="35.0" layoutY="156.0" nodeOrientation="LEFT_TO_RIGHT" text="Password">
		<font>
			<Font name="Corbel" size="14.0"/>
		</font>
	</Label>
	<TextField fx:id="unameBox" layoutX="104.0" layoutY="103.0" nodeOrientation="LEFT_TO_RIGHT"
	           prefHeight="26.0" prefWidth="166.0">
		<font>
			<Font name="Arial" size="14.0"/>
		</font>
	</TextField>
	<PasswordField fx:id="pwdBox" layoutX="104.0" layoutY="152.0" nodeOrientation="LEFT_TO_RIGHT">
		<font>
			<Font name="Corbel" size="14.0"/>
		</font>
	</PasswordField>
	<Button fx:id="loginBtn" layoutX="157.0" layoutY="207.0" mnemonicParsing="false"
	        nodeOrientation="LEFT_TO_RIGHT" onAction="#loginBtnAction" text="Sign In">
		<font>
			<Font name="Corbel" size="14.0"/>
		</font>
	</Button>
</Pane>
