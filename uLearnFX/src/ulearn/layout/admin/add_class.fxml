<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?><?import javafx.scene.control.*?><?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
    fx:controller="ulearn.controllers.ToolCont" prefHeight="400.0" prefWidth="600.0">
  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
    <GridPane prefHeight="370.8" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0" minWidth="10.0" prefWidth="295.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="307.0" minWidth="10.0" prefWidth="305.0"/>
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
      </rowConstraints>
      <TextField fx:id="txtNewClassName" maxWidth="1.7976931348623157E308" minHeight="25.0" minWidth="-Infinity"
          promptText="Class Name: {I.E. Software Engineering}">
        <GridPane.margin>
          <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
        </GridPane.margin>
        <padding>
          <Insets left="10.0" right="10.0"/>
        </padding>
      </TextField>
      <TextField fx:id="txtNewClassId" maxWidth="1.7976931348623157E308" minHeight="25.0"
          promptText="Class ID: {I.E. CS-201, CS-347, or ECON-101}" GridPane.rowIndex="1">
        <padding>
          <Insets left="10.0" right="10.0"/>
        </padding>
        <GridPane.margin>
          <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
        </GridPane.margin>
      </TextField>
      <TextField fx:id="txtNewClassEdu" maxWidth="1.7976931348623157E308" minHeight="25.0"
          promptText="Educator's Name: {I.E. Vineyard}" GridPane.rowIndex="2">
        <padding>
          <Insets left="10.0" right="10.0"/>
        </padding>
        <GridPane.margin>
          <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
        </GridPane.margin>
      </TextField>
      <TextField fx:id="txtNewClassSize" maxWidth="1.7976931348623157E308" minHeight="25.0"
          promptText="Max Class Size: {I.E. 20, 25 ,30}" GridPane.rowIndex="3">
        <padding>
          <Insets left="10.0" right="10.0"/>
        </padding>
        <GridPane.margin>
          <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
        </GridPane.margin>
      </TextField>
      <Button fx:id="newClassBtn" mnemonicParsing="false" onAction="#makeNewUser" prefWidth="125.0" text="Create Class"
          GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4"/>
    </GridPane>
  </AnchorPane>
</AnchorPane>
