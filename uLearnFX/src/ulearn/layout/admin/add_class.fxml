  <GridPane prefHeight="370.8" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="302.0" minWidth="10.0" prefWidth="297.0"/>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="356.0" minWidth="10.0" prefWidth="303.0"/>
    </columnConstraints>
    <rowConstraints>
      <RowConstraints maxHeight="90.0" minHeight="10.0" prefHeight="90.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="95.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="91.0" minHeight="10.0" prefHeight="44.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="88.0" minHeight="10.0" prefHeight="44.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="81.0" minHeight="10.0" prefHeight="43.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="122.0" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES"/>
      <RowConstraints maxHeight="122.0" minHeight="10.0" prefHeight="55.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <TextField fx:id="txtNewClassName" maxWidth="1.7E308" minHeight="25.0" minWidth="-Infinity"
        promptText="Class Name (I.E. Software Engineering)" GridPane.rowIndex="1" GridPane.valignment="TOP">
      <GridPane.margin>
        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
      </GridPane.margin>
      <padding>
        <Insets left="10.0" right="10.0"/>
      </padding>
    </TextField>
    <TextField fx:id="txtNewClassId" maxWidth="1.7E308" minHeight="25.0"
        promptText="New Class ID (I.E. CS-203, CS-471)" GridPane.rowIndex="2" GridPane.valignment="TOP">
      <padding>
        <Insets left="10.0" right="10.0"/>
      </padding>
      <GridPane.margin>
        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
      </GridPane.margin>
    </TextField>
    <TextField fx:id="txtNewEduName" maxWidth="1.7E308" minHeight="25.0"
        promptText="Name of Educator (I.E. Vineyard)" GridPane.rowIndex="3" GridPane.valignment="TOP">
      <padding>
        <Insets left="10.0" right="10.0"/>
      </padding>
      <GridPane.margin>
        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
      </GridPane.margin>
    </TextField>
    <TextField fx:id="txtNewSeats" maxWidth="1.7E308" minHeight="25.0"
        promptText="Maximum Available Seats (I.E. 25, 30)" GridPane.rowIndex="4" GridPane.valignment="TOP">
