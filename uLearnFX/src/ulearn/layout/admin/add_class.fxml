        <Insets left="10.0" right="10.0"/>
      </padding>
      <GridPane.margin>
        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
      </GridPane.margin>
    </TextField>
    <Button fx:id="newUserBtn" mnemonicParsing="false" onAction="#makeNewUser" prefWidth="100.0" text="Create Class"
        GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="4" GridPane.valignment="TOP">
      <GridPane.margin>
        <Insets bottom="10.0" left="30.0" right="10.0" top="10.0"/>
      </GridPane.margin>
    </Button>
    <Label maxHeight="1.7E308" maxWidth="1.7E308" text="Create Class">
      <font>
        <Font name="Droid Sans Bold" size="24.0"/>
      </font>
      <padding>
        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
      </padding>
    </Label>
    <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
